  <xs:import namespace="http://www.w3.org/XML/1998/namespace"></xs:import>
  <xs:import namespace="http://www.springframework.org/schema/beans" schemaLocation="http://www.springframework.org/schema/beans/spring-beans.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/core" schemaLocation="http://www.mulesoft.org/schema/mule/core/current/mule.xsd"></xs:import>
  <xs:import namespace="http://www.mulesoft.org/schema/mule/tls" schemaLocation="http://www.mulesoft.org/schema/mule/tls/current/mule-tls.xsd"></xs:import>
  <xs:complexType name="com.mulesoft.connector.netsuite.api.ConcurrencyStrategyConfiguration">
    <xs:complexContent>
      <xs:extension xmlns:mule="http://www.mulesoft.org/schema/mule/core" base="mule:abstractExtensionType"></xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" xmlns:netsuite="http://www.mulesoft.org/schema/mule/netsuite" type="netsuite:com.mulesoft.connector.netsuite.api.ConcurrencyStrategyConfiguration" substitutionGroup="mule:abstract-shared-extension" abstract="true" name="global-abstract-concurrency-strategy-configuration"></xs:element>
  <xs:element xmlns:netsuite="http://www.mulesoft.org/schema/mule/netsuite" type="netsuite:com.mulesoft.connector.netsuite.api.ConcurrencyStrategyConfiguration" abstract="true" name="abstract-concurrency-strategy-configuration"></xs:element>
