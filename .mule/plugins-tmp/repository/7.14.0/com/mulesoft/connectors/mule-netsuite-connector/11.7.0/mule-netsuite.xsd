    <xs:complexContent>
      <xs:extension xmlns:netsuite="http://www.mulesoft.org/schema/mule/netsuite" base="netsuite:com.mulesoft.connector.netsuite.api.ConcurrencyStrategyConfiguration">
        <xs:attribute xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="mule:expressionInt" use="optional" default="200" name="maxRetries"></xs:attribute>
        <xs:attribute xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="mule:expressionInt" use="optional" default="64000" name="maxWaitingTimeBetweenRetries"></xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
